{
  "name": "mrm-preset-rmr",
  "version": "2.1.0",
  "description": "RMR preset for mrm",
  "main": "config.json",
  "files": [
    "config.json",
    "*/index.js"
  ],
  "keywords": [
    "mrm",
    "mrm-task",
    "mrm-preset"
  ],
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.1.0",
    "lint-staged": "^9.5.0",
    "mrm-core": "^3.3.4",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n1zee/rmr-auto-config.git"
  },
  "contributors": [
    "n1zee",
    {
      "name": "Sergey Sova",
      "email": "work@sergeysova.com",
      "url": "https://sova.dev"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/n1zee/rmr-auto-config/issues"
  },
  "homepage": "https://github.com/n1zee/rmr-auto-config#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
